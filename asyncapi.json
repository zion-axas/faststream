            "exclusive": false,
            "autoDelete": false,
            "vhost": "/"
          },
          "exchange": {
            "type": "default",
            "vhost": "/"
          }
        }
      },
      "subscribe": {
        "bindings": {
          "amqp": {
            "cc": "test1",
            "ack": true,
            "bindingVersion": "0.2.0"
          }
        },
        "message": {
